# API Configuration
VITE_API_URL=/api/v1

# Authentication
VITE_AUTH_ENABLED=true
VITE_GOOGLE_CLIENT_ID=your-google-client-id-prod
VITE_FACEBOOK_APP_ID=your-facebook-app-id-prod

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_LOGGING=false

# UI Settings
VITE_THEME=system
VITE_DEFAULT_LANGUAGE=en

# Production Settings
VITE_DEBUG=false
VITE_DEV_TOOLS=false

# Security Headers
VITE_CSP_HEADER="default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://www.google-analytics.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self'; connect-src 'self' https://api.example.com;"

# Performance
VITE_LAZY_LOAD_ROUTES=true
VITE_ENABLE_PRECOMPRESSION=true
